{
  "name": "scrimhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "studio": "npx prisma studio",
    "init": "npm run generate",
    "docker:migrate": "docker-compose --env-file ./.env.local exec scrimhub-client npm run migrate",
    "docker:generate": "docker-compose --env-file ./.env.local exec scrimhub-client npm run generate",
    "docker:init": "docker-compose exec scrimhub-client npm run init",
    "docker:dev": "docker-compose --env-file ./.env.local --profile development up",
    "docker:stop": "docker-compose --env-file ./.env.local stop",
    "docker:build": "docker-compose --env-file ./.env.local build",
    "docker:down": "docker-compose --env-file ./.env.local down -v",
    "docker:studio": "docker-compose --env-file ./.env.local exec scrimhub-client npm run studio",
    "docker:install": "docker-compose --env-file ./.env.local exec scrimhub-client npm i"
  },
  "dependencies": {
    "@apollo/client": "^3.6.7",
    "@graphql-tools/schema": "^8.3.14",
    "@prisma/client": "^3.15.1",
    "apollo-server-micro": "^3.8.2",
    "bcrypt": "^5.0.1",
    "cookies": "^0.8.0",
    "graphql-constraint-directive": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "micro-cors": "^0.1.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "swr": "^1.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookies": "^0.7.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "17.0.42",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/uuid": "^8.3.4",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "prisma": "^3.15.1",
    "typescript": "4.7.3"
  }
}
