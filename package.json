{
  "name": "scrimhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "studio": "npx prisma studio",
    "init": "npm run generate",
    "docker:migrate": "docker-compose exec scrimhub-client npm run migrate",
    "docker:init": "docker-compose exec scrimhub-client npm run init",
    "docker:dev": "docker-compose up",
    "docker:build": "docker-compose build",
    "docker:down": "docker-compose down -v",
    "docker:studio": "docker-compose exec scrimhub-client npm run studio"
  },
  "dependencies": {
    "@apollo/client": "^3.6.7",
    "@prisma/client": "^3.15.1",
    "apollo-server-micro": "^3.8.2",
    "micro-cors": "^0.1.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/micro-cors": "^0.1.2",
    "@types/node": "17.0.42",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "prisma": "^3.15.1",
    "typescript": "4.7.3"
  }
}
